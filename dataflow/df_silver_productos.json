{
	"name": "df_silver_productos",
	"properties": {
		"folder": {
			"name": "SILVER"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_productos",
						"type": "DatasetReference"
					},
					"name": "productos"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_productos_cat_mant",
						"type": "DatasetReference"
					},
					"name": "productoscatmant"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_sql_silver_productos",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "drvcatidnormaliza"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "join1"
				},
				{
					"name": "cast1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          prd_id as string,",
				"          prd_key as string,",
				"          prd_nm as string,",
				"          prd_cost as string,",
				"          prd_line as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> productos",
				"source(output(",
				"          ID as string,",
				"          CAT as string,",
				"          SUBCAT as string,",
				"          MAINTENANCE as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> productoscatmant",
				"productoscatmant derive(id_norm = upper(trim(ID))) ~> drvcatidnormaliza",
				"productos derive(id_norm_left = upper( substring(replace(prd_key, '-', '_'), 0, 5) )\r",
				"\r",
				") ~> derivedColumn1",
				"derivedColumn1, drvcatidnormaliza join(toString(trim(id_norm_left)) == toString(trim(id_norm)),",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1 cast(output(",
				"          prd_id as integer,",
				"          prd_key as string,",
				"          prd_nm as string,",
				"          prd_cost as decimal(10,2),",
				"          prd_line as string,",
				"          CAT as string,",
				"          SUBCAT as string,",
				"          MAINTENANCE as string",
				"     ),",
				"     errors: true) ~> cast1",
				"cast1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          prd_id as integer,",
				"          prd_key as string,",
				"          prd_nm as string,",
				"          prd_cost as decimal(18,2),",
				"          prd_line as string,",
				"          cat_id as string,",
				"          categoria as string,",
				"          subcategoria as string,",
				"          maintenance as boolean",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          prd_id,",
				"          prd_key,",
				"          prd_nm,",
				"          prd_cost,",
				"          prd_line,",
				"          cat_id = CAT,",
				"          subcategoria = SUBCAT,",
				"          maintenance = MAINTENANCE",
				"     )) ~> sink1"
			]
		}
	}
}
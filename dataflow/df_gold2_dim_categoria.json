{
	"name": "df_gold2_dim_categoria",
	"properties": {
		"folder": {
			"name": "GOLD"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_productos_cat_mant",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_sql_gold2_dim_categoria",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ID as string,",
				"          CAT as string,",
				"          SUBCAT as string,",
				"          MAINTENANCE as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source1 select(mapColumn(",
				"          codigo = ID,",
				"          categoria = CAT,",
				"          subcategoria = SUBCAT,",
				"          maintenance_raw = MAINTENANCE",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 derive(maintenance = iif(upper(trim(maintenance_raw)) == 'YES', true(), false())) ~> derivedColumn1",
				"derivedColumn1 aggregate(groupBy(codigo,",
				"          categoria,",
				"          subcategoria,",
				"          maintenance),",
				"     cnt_tmp = count(1)) ~> aggregate1",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id_categoria as integer,",
				"          codigo as string,",
				"          categoria as string,",
				"          subcategoria as string,",
				"          maintenance as boolean",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          codigo,",
				"          categoria,",
				"          subcategoria,",
				"          maintenance",
				"     )) ~> sink1"
			]
		}
	}
}